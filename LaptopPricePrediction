\n    'Screen'\n]\n\n# Loop through each feature and print the number of unique values\nfor feature in features_to_check:\n    print(f\"{feature}: {df_test[feature].nunique()}\")\n\n# List of features to be target encoded\nfeatures_to_encode = [\n    'Model Name',\n    'Category',\n    'Manufacturer',\n    'Operating System',\n    'Storage_Type',\n    'GPU',\n    'CPU_Model',\n    'Screen'\n]\n\n# Since target encoding should be based on training data, you'd usually use the \n# training data's mean values. For simplicity, here we assume the same operation:\n# (Be cautious of data leakage in a real scenario)\nfor feature in features_to_encode:\n    encoded_feature_name = f'{feature}_Encoded'\n    df_test[encoded_feature_name] = df_test.groupby(feature)['Price'].transform('mean')\n\n    \n\ndf_test = df_test.drop(columns=[ \"CPU\", \" Storage\", 'Model Name', 'Operating System', 'Category', 'Manufacturer', 'Screen', 'GPU', 'CPU_Model', 'Storage_Type'], axis=1)\n\n# Scaling the features\nscaler = MinMaxScaler()\ndf_test_scaled = scaler.fit_transform(df_test)\n\n# Convert the scaled NumPy array back to a DataFrame\ncolumns = df_test.columns\ndf_test = pd.DataFrame(df_test_scaled, columns=columns)\n\ndf_test.head()\n\n\n\n","metadata":{"execution":{"iopub.status.busy":"2024-08-17T14:54:00.093214Z","iopub.execute_input":"2024-08-17T14:54:00.094267Z","iopub.status.idle":"2024-08-17T14:54:00.260721Z","shell.execute_reply.started":"2024-08-17T14:54:00.094200Z","shell.execute_reply":"2024-08-17T14:54:00.259286Z"},"trusted":true},"execution_count":27,"outputs":[{"name":"stdout","text":"CPU: 50\n Storage: 22\nModel Name: 197\nOperating System: 6\nCategory: 6\nManufacturer: 12\nGPU: 54\nStorage_Type: 7\nScreen: 21\n","output_type":"stream"},{"execution_count":27,"output_type":"execute_result","data":{"text/plain":"   Screen Size       RAM    Weight     Price  CPU_Speed  Storage_Capacity  \\\n0     0.763889  0.064516  0.345269  0.106551   0.666667          0.499756   \n1     1.00